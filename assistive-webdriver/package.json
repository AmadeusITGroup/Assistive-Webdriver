    "pngjs": "5.0.0",
    "request": "2.88.2",
    "request-promise-native": "1.0.8",
    "stream-json": "1.5.0",
    "uuid": "8.0.0",
    "virtualbox-soap": "3.0.3",
    "winston": "3.2.1",
    "ws": "7.2.5",
    "yargs": "15.3.1"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "4.1.1",
    "@types/co-body": "5.1.0",
    "@types/http-proxy": "1.17.4",
    "@types/jest": "25.2.1",
    "@types/koa": "2.11.3",
    "@types/koa-bodyparser": "4.3.0",
    "@types/koa-route": "3.2.4",
    "@types/pngjs": "3.4.2",
    "@types/request": "2.48.4",
    "@types/request-promise-native": "1.0.17",
    "@types/selenium-webdriver": "4.0.9",
    "@types/stream-json": "1.0.1",
    "@types/uuid": "7.0.3",
    "@types/ws": "7.2.4",
    "@types/yargs": "15.0.4",
    "@typescript-eslint/eslint-plugin": "2.30.0",
    "@typescript-eslint/parser": "2.30.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "jest": "25.5.1",
    "lolex": "6.0.0",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "rollup": "2.7.5",
    "selenium-webdriver": "4.0.0-alpha.7",
    "ts-jest": "25.4.0",
    "typescript": "3.8.3"
  },
  "peerDependencies": {
    "selenium-webdriver": "*"
  },
  "scripts": {
    "prepare": "yarn build",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "postbuild": "tsc -p tsconfig.d.json",
    "format": "prettier '*.{ts,js,json}' '{src,server,test,samples}/**/*.{ts,js,json}'",
    "format:check": "npm run format -- --check",
    "format:fix": "npm run format -- --write",
    "lint": "eslint '{src,server,test,samples}/**/*.{ts,js,json}'",
    "start": "node dist/server/bin.js",
    "test": "jest",
    "tester": "node dist/tester.js"
  }
