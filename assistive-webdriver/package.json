{
  "name": "assistive-webdriver",
  "version": "0.0.0",
  "description": "Assistive-Webdriver is a tool to automate end-to-end web application tests with a screen reader.",
  "bin": "assistive-webdriver",
  "main": "dist/client/index.js",
  "module": "dist/client/index.mjs",
  "typings": "dist/client/index.d.ts",
  "author": "David-Emmanuel Divernois <david-emmanuel.divernois@amadeus.com>",
  "homepage": "https://github.com/AmadeusITGroup/Assistive-Webdriver",
  "bugs": "https://github.com/AmadeusITGroup/Assistive-Webdriver/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/AmadeusITGroup/Assistive-Webdriver.git",
    "directory": "tcp-web-listener"
  },
  "keywords": [
    "automation",
    "selenium",
    "testing",
    "webdriver",
    "webdriverjs",
    "screen readers",
    "a11y",
    "accessibility"
  ],
  "files": [
    "assistive-webdriver",
    "dist",
    "server"
  ],
  "license": "MIT",
  "dependencies": {
    "co-body": "6.0.0",
    "colors": "1.4.0",
    "http-proxy": "1.18.1",
    "koa": "2.13.0",
    "koa-route": "3.2.0",
    "node-fetch": "2.6.0",
    "p-queue": "6.6.0",
    "pngjs": "5.0.0",
    "stream-json": "1.7.1",
    "uuid": "8.3.0",
    "virtualbox-soap": "3.0.3",
    "winston": "3.3.3",
    "ws": "7.3.1",
    "yargs": "15.4.1"
  },
  "devDependencies": {
    "@babel/core": "7.11.1",
    "@babel/plugin-syntax-typescript": "7.10.4",
    "@rollup/plugin-typescript": "5.0.2",
    "@types/co-body": "5.1.0",
    "@types/http-proxy": "1.17.4",
    "@types/jest": "26.0.9",
    "@types/node-fetch": "2.5.7",
    "@types/koa": "2.11.4",
    "@types/koa-bodyparser": "4.3.0",
    "@types/koa-route": "3.2.4",
    "@types/pngjs": "3.4.2",
    "@types/selenium-webdriver": "4.0.9",
    "@types/stream-json": "1.5.1",
    "@types/uuid": "8.3.0",
    "@types/ws": "7.2.6",
    "@types/yargs": "15.0.5",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "babel-plugin-istanbul": "6.0.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "jest": "25.5.4",
    "lolex": "6.0.0",
    "nyc": "15.1.0",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "rollup": "2.26.4",
    "selenium-webdriver": "4.0.0-alpha.7",
    "ts-jest": "25.5.1",
    "typescript": "3.9.7"
  },
  "peerDependencies": {
    "selenium-webdriver": "*"
  },
  "scripts": {
    "prepare": "yarn build",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "postbuild": "tsc -p tsconfig.d.json",
    "format": "prettier '*.{ts,js,json}' '{src,server,test,samples}/**/*.{ts,js,json}'",
    "format:check": "npm run format -- --check",
    "format:fix": "npm run format -- --write",
    "lint": "eslint '{src,server,test,samples}/**/*.{ts,js,json}'",
    "start": "node dist/server/bin.js",
    "test": "jest",
    "tester": "node dist/tester.js",
    "nyc-tester": "nyc node dist/tester.js",
    "nyc-server": "nyc node dist/server/bin.js",
    "nyc-report": "nyc report --reporter=lcov"
  },
  "nyc": {
    "instrument": false,
    "sourceMap": false,
    "silent": true,
    "clean": false
  }
}
